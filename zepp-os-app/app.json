{
  "configVersion": "v3.0",
  "app": {
    "appId": 1088977,
    "appName": "BioPeak Sync",
    "package": "com.biopeak.sync",
    "appType": "app",
    "version": {
      "code": 2,
      "name": "1.1.0"
    },
    "icon": "icon.png",
    "vendor": "BioPeak",
    "description": "Sync your workouts with BioPeak AI platform"
  },
  "permissions": [
    "data:user.info",
    "data:exercise",
    "data:heartrate",
    "data:steps",
    "device:bluetooth",
    "device:os.bg_service"
  ],
  "runtime": {
    "apiVersion": {
      "minVersion": "2.0",
      "target": "3.6",
      "compatible": "3.6"
    }
  },
  "pages": ["page/index"],
  "module": {
    "page": {
      "pages": ["page/index"]
    },
    "app-side": {
      "path": "app-side/index"
    }
  },
  "defaultLanguage": "en-US",
  "i18n": {
    "en-US": {
      "name": "BioPeak Sync"
    },
    "pt-BR": {
      "name": "BioPeak Sync"
    }
  },
  "targets": {
    "gtr_4": {
      "module": {
        "page": {
          "pages": ["page/index"]
        },
        "app-side": {
          "path": "app-side/index"
        }
      },
      "platforms": [
        {
          "name": "pamir",
          "deviceSource": 7995648
        }
      ]
    },
    "gts_4": {
      "module": {
        "page": {
          "pages": ["page/index"]
        },
        "app-side": {
          "path": "app-side/index"
        }
      },
      "platforms": [
        {
          "name": "snowy",
          "deviceSource": 7995649
        }
      ]
    }
  }
}