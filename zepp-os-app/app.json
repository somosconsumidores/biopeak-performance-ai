{
  "app": {
    "appId": 1013562,
    "appName": "BioPeak Sync",
    "appType": "app",
    "version": {
      "code": 1,
      "name": "1.0.0"
    },
    "icon": "icon.png",
    "vender": "BioPeak",
    "description": "Sync your workouts with BioPeak AI platform"
  },
  "permissions": [
    "data:user_data.heart_rate",
    "data:user_data.step",
    "data:user_data.distance",
    "data:user_data.calorie",
    "device:bluetooth",
    "device:os.bg_service"
  ],
  "runtime": {
    "apiVersion": {
      "minVersion": "3.0",
      "targetVersion": "3.6"
    }
  },
  "pages": [
    "page/index"
  ],
  "module": {
    "page": {
      "pages": [
        "page/index"
      ]
    },
    "app-side": {
      "path": "app-side/index"
    }
  },
  "i18n": {
    "en-US": {
      "name": "BioPeak Sync"
    },
    "pt-BR": {
      "name": "BioPeak Sync"  
    }
  },
  "defaultLanguage": "en-US"
}